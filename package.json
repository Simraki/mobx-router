{
  "name": "@simraki/mobx-router",
  "version": "0.3.0",
  "description": "A simple router for MobX",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "types/",
    "dist/",
    "src/",
    "test/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "test": "jest",
    "build": "del-cli dist && tsc",
    "format": "prettier --print-width 80 --tab-width 4 --single-quote --write {src,tests}/**/*.{ts,tsx}",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simraki/mobx-router.git"
  },
  "keywords": [
    "Simraki",
    "mobx",
    "router",
    "react"
  ],
  "license": "ISC",
  "homepage": "https://github.com/Simraki/mobx-router#readme",
  "dependencies": {
    "director": "1.2.8",
    "query-string": "^7.0.1"
  },
  "peerDependencies": {
    "mobx": "^6.x",
    "mobx-react-lite": "^3.x",
    "react": "^17"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.0",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "del-cli": "^4.0.1",
    "eslint": "^7.32.0",
    "husky": "^7.0.2",
    "jest": "^27.2.2",
    "lint-staged": "^11.1.2",
    "mobx": "^6.3.3",
    "mobx-react-lite": "^3.2.1",
    "prettier": "^2.4.1",
    "raf": "^3.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "npm run format",
      "git add"
    ]
  },
  "packageManager": "yarn@3.0.2"
}
